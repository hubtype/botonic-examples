{
  "name": "botonic-booking-platform",
  "version": "1.0.0",
  "scripts": {
    "build": "webpack --env target=all --mode=production",
    "start": "webpack serve --env target=dev --mode=development",
    "test": "jest",
    "train": "node_modules/.bin/ts-node src/nlu/train-model.ts"
  },
  "jest": {
    "rootDir": "tests",
    "transformIgnorePatterns": [
      "/node_modules/(?!@botonic|react-children-utilities).+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(scss|css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@botonic/react": "0.17.0-alpha.3",
    "@date-io/date-fns": "^1.3.13",
